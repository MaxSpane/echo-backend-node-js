{
  "name": "echo-backend-node",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "local-db:refresh": "export NODE_ENV=test && babel-node server/db/migrations dropTables && babel-node server/db/migrations createTables && babel-node server/db/migrations migrateTables && babel-node server/db/seeder seedTables",
    "local-test": "npm run local-db:refresh && npm run test",
    "build": "rimraf build/ && babel ./ -d build/ --ignore \"./node_modules/,./coverage/,./.babelrc,./.nyc_output/,./package.json,./package-lock.json,./npm-debug.log,./.eslintrc.js,./README.md,./server/db/migrations.js,./server/db/queries.js,./server/db/seeder.js\"",
    "start": "node build/app.js",
    "dev": "nodemon app.js --exec babel-node --",
    "heroku-postbuild": "babel-node server/db/migrations dropTables && babel-node server/db/migrations createTables && babel-node server/db/seeder seedTables"
  },
